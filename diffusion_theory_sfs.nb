Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"Data1s", "=", 
     RowBox[{"Transpose", "@", 
      RowBox[{"{", 
       RowBox[{"xaxis", ",", 
        RowBox[{"xaxis", " ", 
         FractionBox["gData", "theta"]}]}], "}"}]}]}], ";"}], 
   "\[IndentingNewLine]", "\n", 
   RowBox[{"(*", 
    RowBox[{
    "Define", " ", "logarithmic", " ", "binning", " ", "and", " ", 
     "smoothing", " ", "function"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"smoothDataLog", "[", 
      RowBox[{"data_", ",", "numBins_", ",", "xThreshold_"}], "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "filteredData", ",", "bins", ",", "binnedData", ",", "binEdges", ",", 
         "binnedX", ",", "binnedY", ",", "counts", ",", "logMin", ",", 
         "logMax", ",", "logBinEdges"}], "}"}], ",", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{
         "Filter", " ", "data", " ", "to", " ", "include", " ", "only", " ", 
          "points", " ", "where", " ", "x"}], ">", "xThreshold"}], "*)"}], 
       RowBox[{
        RowBox[{"filteredData", "=", 
         RowBox[{"Select", "[", 
          RowBox[{"data", ",", 
           RowBox[{
            RowBox[{
             RowBox[{"#", "[", 
              RowBox[{"[", "1", "]"}], "]"}], ">", "xThreshold"}], "&"}]}], 
          "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
         "Determine", " ", "the", " ", "range", " ", "and", " ", "width", " ",
           "of", " ", "bins", " ", "for", " ", "filtered", " ", "data"}], 
         "*)"}], 
        RowBox[{"logMin", "=", 
         RowBox[{"Log10", "[", 
          RowBox[{"Min", "[", 
           RowBox[{"filteredData", "[", 
            RowBox[{"[", 
             RowBox[{"All", ",", "1"}], "]"}], "]"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"logMax", "=", 
         RowBox[{"Log10", "[", 
          RowBox[{"Max", "[", 
           RowBox[{"filteredData", "[", 
            RowBox[{"[", 
             RowBox[{"All", ",", "1"}], "]"}], "]"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"logBinEdges", "=", 
         RowBox[{"Range", "[", 
          RowBox[{"logMin", ",", "logMax", ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"logMax", "-", "logMin"}], ")"}], "/", "numBins"}]}], 
          "]"}]}], ";", "\[IndentingNewLine]", 
