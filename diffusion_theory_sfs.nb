Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"N1", "=", "1000"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"mu", "=", 
     RowBox[{"10", "^", 
      RowBox[{"-", "7"}]}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"L", "=", "100000"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"theta", "=", 
     RowBox[{"4", " ", "N1", " ", "mu", " ", "L"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"N1", "=", "1000"}], ";", 
    RowBox[{"s", "=", "0.1"}], ";", 
    RowBox[{"h", "=", "0.2"}], ";", 
    RowBox[{"x0", "=", 
     RowBox[{"1", "/", 
      RowBox[{"(", 
       RowBox[{"2", " ", "2", " ", "h", " ", "s", " ", "N1"}], ")"}]}]}], 
    ";"}], "\n", 
   RowBox[{
    RowBox[{"x1", "=", 
     RowBox[{"NDSolveValue", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{"x2a", "[", "t", "]"}], ",", "t"}], "]"}], "==", 
          RowBox[{"s", "*", 
           RowBox[{"x2a", "[", "t", "]"}], "*", 
           RowBox[{"(", 
            RowBox[{"1", "-", 
             RowBox[{"x2a", "[", "t", "]"}]}], ")"}], " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"x2a", "[", "t", "]"}], " ", "+", " ", 
             RowBox[{"h", " ", 
              RowBox[{"(", 
               RowBox[{"1", "-", 
                RowBox[{"2", " ", 
                 RowBox[{"x2a", "[", "t", "]"}]}]}], ")"}]}]}], ")"}]}]}], 
         ",", 
         RowBox[{
          RowBox[{"x2a", "[", "0", "]"}], "==", "x0"}]}], "}"}], ",", "x2a", 
       ",", 
       RowBox[{"{", 
        RowBox[{"t", ",", "0", ",", "1500"}], "}"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\n", 
   RowBox[{"tfix", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"Log", "[", 
       RowBox[{"2", " ", "2", " ", "N1", " ", "s"}], "  ", "]"}], 
      RowBox[{"h", "  ", "s", " ", 
       RowBox[{"(", 
        RowBox[{"1", "-", "h"}], ")"}]}]], "+", " ", 
     FractionBox[
      RowBox[{"EulerGamma", "+", " ", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"2", "-", " ", 
          RowBox[{"3", " ", "h"}]}], ")"}], 
        RowBox[{"Log", "[", "h", "]"}]}], " ", "+", " ", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"3", " ", "h"}], "-", "1"}], ")"}], " ", 
        RowBox[{"Log", "[", 
         RowBox[{"1", "-", "h"}], "]"}]}]}], 
      RowBox[{"h", "  ", "s", " ", 
       RowBox[{"(", 
        RowBox[{"1", "-", "h"}], ")"}]}]]}]}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"G", "=", 
     RowBox[{"NDSolveValue", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{"g", "[", 
             RowBox[{"x", ",", "t"}], "]"}], ",", "t"}], "]"}], "==", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{"x", " ", 
              RowBox[{"(", 
               RowBox[{"1", "-", "x"}], ")"}]}], ")"}], "/", 
            RowBox[{"(", 
             RowBox[{"4", " ", "N1", "*", 
              RowBox[{"x1", "[", "t", "]"}]}], ")"}]}], " ", 
           RowBox[{"D", "[", 
            RowBox[{
             RowBox[{"g", "[", 
              RowBox[{"x", ",", "t"}], "]"}], ",", "x", ",", "x"}], "]"}]}]}],
          ",", 
         RowBox[{
          RowBox[{"g", "[", 
           RowBox[{"1", ",", "t"}], "]"}], "==", "0"}], ",", 
         RowBox[{
          RowBox[{"g", "[", 
           RowBox[{"0", ",", "t"}], "]"}], "==", 
          RowBox[{
           RowBox[{"x1", "[", "t", "]"}], " ", "theta", " ", 
           RowBox[{"(", 
            RowBox[{"1", "-", 
             RowBox[{"Exp", "[", 
              RowBox[{
               RowBox[{"-", "10000000"}], " ", "t"}], "]"}]}], ")"}]}]}], ",", 
         RowBox[{
          RowBox[{"g", "[", 
           RowBox[{"x", ",", "0"}], "]"}], "==", "0"}]}], "}"}], ",", "g", 
       ",", 
       RowBox[{"{", 
        RowBox[{"t", ",", "0", ",", "1500"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "0", ",", "1"}], "}"}]}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"gData", "=", 
     RowBox[{"Table", "[", " ", 
      RowBox[{
       RowBox[{
